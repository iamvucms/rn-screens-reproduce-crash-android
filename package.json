  "scripts": {
    "start": "react-native start -- --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "patch-package && npx jetify",
    "ios": "react-native run-ios --simulator='iPhone 11'",
    "android": "react-native run-android",
    "android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "android-release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean -f && yarn install && yarn start -- --reset-cache",
    "storybook": "start-storybook -p 7007",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "adb-devices": "adb devices",
    "clean-xcode": "cd ios && bash ./resetXcode.sh && cd ..",
    "pod-install": "cd ios && rm Podfile.lock && pod install && cd ..",
    "codepush": "appcenter codepush release-react -a ProjectX/ProjectX -t '=1.14.17' -d Production",
    "codepush-clear": "appcenter codepush deployment clear -a ProjectX/ProjectX Production",
    "codepush-android": "appcenter codepush release-react -a ProjectX/ProjectX-1 -t '=16.0' -d Production",
    "generate:universal:apk": "unzip -p android/app/release/app-release.apks universal.apk > android/app/release/app-release.apk",
    "generate:apks:debug": "bundletool build-apks --bundle=android/app/release/app-release.aab --output=android/app/release/app-debug.apks",
    "find-crashes": "xcrun simctl spawn booted log stream --level debug --style compact | grep -i crash",
    "generate:apks:release": "rm -rf android/app/release/app-release.{apks,apk} && bundletool build-apks --bundle=android/app/release/app-release.aab --output=android/app/release/app-release.apks --mode=universal --ks=android/app/fc-upload-keystore.jks --ks-key-alias=fc-upload-key --ks-pass=file:android/app/keystore.pwd && yarn generate:universal:apk"
  },
